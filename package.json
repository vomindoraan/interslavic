{
  "name": "interslavic",
  "version": "1.1.0",
  "license": "MIT",
  "description": "Interslavic dictionary",
  "author": "Sergey Cherebedov",
  "scripts": {
    "start": "npm run dev",
    "test": "jest --logHeapUsage --coverage",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "lint": "tslint -c tslint.json '{src,tests}/**/*.{ts,tsx}' -t stylish --fix",
    "updateTestSnapshot": "ts-node -r tsconfig-paths/register ./tests/updateTestSnapshot.ts",
    "generateDictionary": "ts-node -r tsconfig-paths/register ./src/services/generateDictionary.ts",
    "build": "cross-env NODE_ENV=production webpack"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "transformIgnorePatterns": [
      "^.+\\.(js|json)$"
    ],
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "verbose": false,
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}"
    ]
  },
  "main": "",
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "@types/classnames": "^2.2.10",
    "@types/dialog-polyfill": "^0.5.0",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.152",
    "@types/node": "^11.15.14",
    "@types/react": "^16.9.35",
    "@types/react-dom": "16.0.9",
    "@types/request": "^2.48.5",
    "acorn": "^6.4.1",
    "autoprefixer": "^9.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "coveralls": "^3.1.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "cssnano": "^4.1.10",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jsdom": "8.5.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "react-transform-hmr": "^1.0.4",
    "request": "^2.88.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-jest": "^24.3.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.14.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.9.3",
    "typings": "^2.1.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.20.0",
    "write-file-webpack-plugin": "^4.5.1"
  }
}
